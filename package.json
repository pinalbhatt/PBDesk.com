{
  "name": "pbdesk-com",
  "version": "1.0.0",
  "description": "PBDesk.com Nuxt Project",
  "author": "Pinal Bhatt",
  "private": true,
  "scripts": {
    "dev": "nuxt --port 4400",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintf": "eslint --quiet --fix --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "predeploy": "nuxt generate",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup"
  },
  "dependencies": {
    "cross-env": "5.2.0",
    "nuxt": "2.3.4",
    "vuetify": "1.3.12",
    "@nuxtjs/axios": "5.3.6"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "eslint": "5.10.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-loader": "2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-vue": "5.0.0",
    "nodemon": "1.18.8",
    "push-dir": "0.4.1",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.2"
  }
}
